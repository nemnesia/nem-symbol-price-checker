{
  "name": "nem-symbol-price-checker",
  "version": "1.0.0",
  "description": "NEM/Symbol cryptocurrency price checker and data collector",
  "author": "ccHarvestasya",
  "type": "module",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "nem",
    "symbol",
    "cryptocurrency",
    "price",
    "tracker",
    "blockchain"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemnesia/nem-symbol-price-checker.git"
  },
  "bugs": {
    "url": "https://github.com/nemnesia/nem-symbol-price-checker/issues"
  },
  "homepage": "https://github.com/nemnesia/nem-symbol-price-checker#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.11.0",
  "volta": {
    "node": "22.21.0",
    "yarn": "4.10.3"
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc --build tsconfig.build.json",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "daily": "tsx src/daily-collector.ts",
    "recovery": "tsx src/recovery.ts",
    "current": "tsx src/current-price.ts",
    "test": "tsx src/test-system.ts",
    "setup": "./setup.sh",
    "uninstall": "./uninstall.sh",
    "lint": "eslint \"{src,test,e2e}/**/*.ts\" --fix",
    "format": "prettier --write \"{src,test,e2e}/**/*.ts\""
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "shx": "^0.4.0",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
